page{
  background: #f5f5f5;
  font-size: 30rpx;
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.hr{
    width: 100%;
    height: 1px;
    margin: 10px 0;
    background-color: #c7c7c7;
}
.hr-this{
  width:60px;
  height:1px;
  margin:10px 0;
  background-color:#9a9a9a;
  display:inline-block;
  position:relative;
  top:5px;
}
